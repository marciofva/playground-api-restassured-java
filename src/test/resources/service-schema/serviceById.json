{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/root.json",
	"type": "object",
	"required": [
		"id",
		"name"
	],
	"properties": {
		"id": {
			"type": "integer"
		},
		"name": {
			"type": "string"
		},
		"createdAt": {
			"type": "string",
			"maxLength": 100,
			"minLength": 1
		},
		"updatedAt": {
			"type": "string"
		}
	}
}